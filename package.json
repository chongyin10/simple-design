{
  "name": "@zjpcy/simple-design",
  "version": "1.0.5",
  "description": "IDP Studio Design System - React Component Library",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "webpack serve --config webpack.config.cjs",
    "build": "rimraf dist && mkdir -p dist && tsc --emitDeclarationOnly --outDir dist/types --project tsconfig.build.json && rollup -c",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types --project tsconfig.build.json",
    "build:rollup": "rollup -c",
    "prebuild": "rimraf dist",
    "lint": "eslint src --ext ts,tsx",
    "preview": "vite preview"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "idp-studio"
  ],
  "author": "IDP Studio Team",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@ant-design/icons-svg": "^4.4.2",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.7",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.23",
    "css-loader": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.32.0",
    "html-webpack-plugin": "^5.6.5",
    "less": "^4.0.0",
    "less-loader": "^11.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-fast-marquee": "^1.6.5",
    "react-icons": "^5.5.0",
    "rimraf": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^3.0.0",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.13.0"
  },
  "dependencies": {
    "classnames": "^2.3.0",
    "prismjs": "^1.30.0",
    "react-color": "^2.19.3",
    "react-syntax-highlighter": "^16.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:chongyin10/simple-design.git"
  }
}
